package array;

public class practica_array {

	public static void main(String[] args) {
/*		Se puede llenar de esta forma un array
		int [] numeros= new int[3];
		numeros [0]= 7;
		numeros [1]= 14;
		numeros [2]= 21; */
		
		int [] numeros= {5,7,8,9}; //O llenarlo asi
		for(int i=0; i<4; i++) {
			System.out.println(numeros[i]);
		}
